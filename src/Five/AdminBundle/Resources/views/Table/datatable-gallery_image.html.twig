                        <option value="">-- All Styles --</option>
                        <option value="rough_chic" {{ cat2 == 'rough_chic' ? 'selected' : '' }}>Rough Chic</option>
                        <option value="classic" {{ cat2 == 'classic' ? 'selected' : '' }}>Classic</option>
